<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".RegisterPage"
    android:id="@+id/RegisterPage"
    android:background="@color/white">


    <ProgressBar
        android:id="@+id/ProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:visibility="gone"
        android:layout_below="@id/CompleteSignUpButton"/>


    <Button
        android:id="@+id/CompleteSignUpButton"
        android:layout_width="268dp"
        android:layout_height="59dp"
        android:layout_below="@id/editTextTextPassword"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="73dp"
        android:backgroundTint="#2792C4"
        android:text="Sign Up" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="176dp"
        android:layout_height="79dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="681dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="24dp"
        app:srcCompat="@drawable/facebutton" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="646dp"
        android:layout_marginEnd="188dp"
        android:layout_marginBottom="120dp"
        android:text="or with" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="184dp"
        android:layout_height="68dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="687dp"
        android:layout_marginEnd="200dp"
        android:layout_marginBottom="30dp"
        app:srcCompat="@drawable/googlebutton" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="184dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="17dp"
        app:srcCompat="@drawable/logo5" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/editTextPhone"
        android:layout_centerVertical="true"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:minHeight="48dp"
        tools:ignore="SmallSp" />

    <EditText
        android:id="@+id/editTextEmailAddress2"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/editTextSurname"
        android:layout_centerVertical="true"

        android:layout_marginTop="46dp"
        android:ems="10"
        android:hint="Email Address"
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        tools:ignore="SmallSp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/editTextEmailAddress2"
        android:layout_centerVertical="true"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:hint="CellPhone Number"
        android:inputType="phone"
        android:minHeight="48dp"
        tools:ignore="SmallSp" />

    <EditText
        android:id="@+id/editTextName"
        android:layout_width="354dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageView2"

        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="23dp"
        android:ems="10"

        android:inputType="text"
        android:minHeight="48dp" />

    <EditText
        android:id="@+id/editTextSurname"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/editTextName"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="27dp"
        android:ems="10"

        android:inputType="text"
        android:minHeight="48dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="189dp"
        android:layout_marginEnd="327dp"
        android:layout_marginBottom="576dp"
        android:text="Sign Up" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="218dp"
        android:layout_marginEnd="305dp"
        android:layout_marginBottom="547dp"
        android:text="First Name"
        android:textColor="#2792C4" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="282dp"
        android:layout_marginEnd="305dp"
        android:layout_marginBottom="483dp"
        android:text="Surname"
        android:textColor="#2792C4" />

</RelativeLayout>