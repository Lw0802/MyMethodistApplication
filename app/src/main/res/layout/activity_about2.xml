        android:id="@+id/textView4"
        android:layout_width="375dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="31dp"
        android:text="The mission of GraceWell Methodist Church is to serve as a beacon of God's grace and love in the community and beyond. The church is committed to fulfilling the teachings of Jesus Christ and the principles of Methodism "
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.73"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.08" />

    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="328dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="403dp"
        android:layout_marginBottom="0dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="89dp"
            android:layout_marginStart="13dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="31dp"
            android:text=" It seeks to foster a deep and meaningful relationship with God through inspiring worship services, Bible study groups, prayer gatherings, and spiritual retreats.."
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.384"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintVertical_bias="0.872" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="356dp"
            android:layout_height="111dp"
            android:layout_marginStart="13dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="212dp"
            android:text=" It seeks to foster a deep and meaningful relationship with God through inspiring worship services, Bible study groups, prayer gatherings, and spiritual retreats.The church is dedicated to equipping its members with the knowledge and understanding of Christian faith through various educational programs. It actively serves the community through outreach programs, addressing social justice issues, and partnering with other organizations. GraceWell Methodist Church strives to create an inclusive and welcoming environment where diversity is embraced."
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.384"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.205" />
    </LinearLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_alignParentBottom="true"
        app:menu="@menu/bottom_nav_bar">
    </com.google.android.material.bottomnavigation.BottomNavigationView>
</RelativeLayout>